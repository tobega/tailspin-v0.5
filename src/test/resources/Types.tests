---autotype number
{ foo: 4 } -> { foo: {} } !
=*TypeError
---autotype big number
{ foo: 20_000_000_000_000_000_000 } -> { foo: {} } !
=*TypeError
---big numbers are also numbers
{ foo: 20_000_000_000_000_000_000 } -> { foo: 4 } !
={ foo: 4 }
---autotype structure
{ foo: { bar: 6 } } -> { foo: 4 } !
=*TypeError
---autotype structure requires all members
{ foo: { bar: 6 } } -> { foo: { qux: 8 } } !
=*TypeError
---autotype structure requires no extra members
{ foo: { bar: 6 } } -> { foo: { bar: 4, qux: 8 } } !
=*TypeError
---autotype structure
{ foo: { bar: 6 } } -> { foo: { bar: 4 } } !
={ foo: { bar: 4 } }
---define type
val requires <|..>
4 -> { val: $ } !
={ val: 4 }
---may not redefine type
val requires <|..>
val requires <|{}>
=*IllegalState
---define type is checked
val requires <|..>
{ val: { foo: 4 } } !
=*TypeError
---define negated type
val requires <~|..3>
4 -> { val: $ } !
={ val: 4 }
---define negated type is checked
val requires <~|..5>
{ val: 4 } !
=*TypeError
---defined type depends on value in defining scope
top is 5;
foo requires <|..$top>
{ foo: 3 } !
={ foo: 3 }
---violation of defined type depending on value in defining scope
top is 5;
foo requires <|..$top>
{ foo: 6 } !
=*TypeError
---is any measure type
1 -> templates
  @ set $;
  2"m" -> !#
  3"s" -> !#
  $@ !
  when <|""> do @ set $@ + 1;
end !
=3
---is not any measure type
1 -> templates
  @ set $;
  2 -> !#
  [5] -> !#
  { foo: 7 } -> !#
  $@ !
  when <|""> do @ set $@ + 1;
end !
=1
---is same measure type
1"m" -> templates
  when <|"m"> do $!
end !
=1"m"
---is not same measure type
1"s" -> templates
  when <|"m"> do $!
  otherwise 0 !
end !
=0
