---is a number
5 -> templates
  when <|..> do 1!
  otherwise 0!
end !
=1
---big number is a number
20_000_000_000_000_000_000 -> templates
  when <|..> do 1!
  otherwise 0!
end !
=1
---structure is not a number
{} -> templates
  when <|..> do 1!
  otherwise 0!
end !
=0
---array is not a number
[1] -> templates
  when <|..> do 1!
  otherwise 0!
end !
=0
---is an array
[2, 3] -> templates
  when <|[]> do 1 !
  otherwise 0 !
end !
=1
---number is not an array
3 -> templates
  when <|[]> do 1 !
  otherwise 0 !
end !
=0
---structure is not an array
{} -> templates
  when <|[]> do 1 !
  otherwise 0 !
end !
=0
---is a structure
{ foo: 1 } -> templates
  when <|{}> do 1!
  otherwise 0!
end !
=1
---number is not a structure
5 -> templates
  when <|{}> do 1!
  otherwise 0!
end !
=0
---array is not a structure
[{foo: 5}] -> templates
  when <|{}> do 1!
  otherwise 0!
end !
=0
---autotype number
{ foo: 4 } -> { foo: {} } !
=*TypeError
---autotype big number
{ foo: 20_000_000_000_000_000_000 } -> { foo: {} } !
=*TypeError
---big numbers are also numbers
{ foo: 20_000_000_000_000_000_000 } -> { foo: 4 } !
={ foo: 4 }
---autotype structure
{ foo: { bar: 6 } } -> { foo: 4 } !
=*TypeError
---autotype structure requires all members
{ foo: { bar: 6 } } -> { foo: { qux: 8 } } !
=*TypeError
---autotype structure requires no extra members
{ foo: { bar: 6 } } -> { foo: { bar: 4, qux: 8 } } !
=*TypeError
---autotype structure
{ foo: { bar: 6 } } -> { foo: { bar: 4 } } !
={ foo: { bar: 4 } }
---define type
val requires <|..>
4 -> { val: $ } !
={ val: 4 }
---may not redefine type
val requires <|..>
val requires <|{}>
=*IllegalState
---define type is checked
val requires <|..>
{ val: { foo: 4 } } !
=*TypeError
